{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Hosting": "Information",
      "Aspire": "Information"
    }
  },
  "AllowedHosts": "*",
  "Aspire": {
    "Resources": {
      "EnableResourceProvisioning": true,
      "EnableHealthChecks": true
    }
  },
  "Infrastructure": {
    "Postgres": {
      "Database": "internationalcenter",
      "EnableHealthCheck": true
    },
    "Redis": {
      "EnableHealthCheck": true
    },
    "RabbitMQ": {
      "EnableHealthCheck": true
    }
  },
  "Services": {
    "ApiGateway": {
      "EnableHealthCheck": true
    },
    "ServicesDomain": {
      "EnableHealthCheck": true
    }
  }
}