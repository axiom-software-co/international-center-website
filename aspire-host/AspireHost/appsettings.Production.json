{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "Aspire": "Warning"
    }
  },
  "Aspire": {
    "Resources": {
      "EnableResourceProvisioning": true,
      "EnableMedicalGradeCompliance": true,
      "EnableAuditLogging": true,
      "EnableSecurityFeatures": true
    }
  },
  "Infrastructure": {
    "Postgres": {
      "EnablePersistentVolumes": true,
      "EnableBackups": true
    }
  }
}